<div class="container">
  <div class="reading-column">    
    <div class="rte">    	
      <div class="wish-desc">
        {% capture page %}{{ settings.select_winpage_content }}{% endcapture %}
         {{ pages[page].content | replace: 'fullwidth"', '" data-fullwidth="true"' }}
      </div>
      <div class="win-form">
        <!-- Begin MailChimp Signup Form -->
        <div id="mc_embed_signup">
          <form action="//floatiekings.us13.list-manage.com/subscribe/post?u=48982d426a910167c31a85aa0&amp;id=bc341c654f&SIGNUP=5000GiveawayUS" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
            <div id="mc_embed_signup_scroll">
              <div class="inner-form">
                <div class="input-row">
                  <div class="mc-field-group">
                    <input type="text" value="" name="FNAME" class="" id="mce-FNAME" placeholder="{{ 'pages.forms.your_name' | t }}">
                  </div>
                </div>
                <div class="input-row">
                  <div class="mc-field-group">
                    <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL" placeholder="{{ 'pages.forms.email' | t }}">
                  </div>
                </div>

                <div id="mce-responses" class="clear">
                  <div class="response" id="mce-error-response" style="display:none"></div>
                  <div class="response" id="mce-success-response" style="display:none"></div>
                </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_48982d426a910167c31a85aa0_bc341c654f" tabindex="-1" value=""></div>
              </div>
              <div class="input-row">
                <div class="clear">
                  <input type="submit" value="{{ 'pages.forms.enter' | t }}" name="subscribe" id="mc-embedded-subscribe" class="button btn enter">
                </div>
              </div>
            </div>
          </form>
        </div>
        <!--End mc_embed_signup-->
      </div>
    </div>
  </div>
</div>